#	cd ./Dist/V5S/RBDEBIT; ${CAPGENPROD} "0001" "123456" "123456" V5S_RBDEBIT 0100 CASTLES 10 10V5S_App V5S_RBDEBIT 0; rm -f *.zip; rm -f *.zip; mv *.mci ./output; mv *.CAP ./output
	cd ./Dist/V5S/${PRONAME}; ${CAPGEN} ${APPNAME} 0001 CASTLES 10 ./10V5S_App ${APPNAME} 0; rm -f *.zip; mv *.mci ./output; mv *.CAP ./output
	
